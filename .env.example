# Example environment variables for mcp-browser-use-server

# Path to Chrome/Chromium executable leave blank to use default playwright chromium
CHROME_PATH=

# OpenAI API key for OpenAI model access
OPENAI_API_KEY=

# Set to true if you want api calls to wait for tasks to complete (default is false)
PATIENT=false

# Set to true if you want to disable anonymous telemetry (default is false)
ANONYMIZED_TELEMETRY=false

# Control headless browser mode used by the server (true = headless, false = visible)
# Default: true
BROWSER_HEADLESS=true

# Maximum agent steps (increase for complex tasks)
MAX_AGENT_STEPS=30

# Run E2E tests
RUN_E2E_TESTS=false

# Logging level controls verbosity. Allowed values:
# - DEBUG: Very verbose, useful for development and troubleshooting
# - INFO: General runtime information (default in production)
# - WARNING: Potential issues to pay attention to
# - ERROR: Errors that prevent some functionality
# - CRITICAL: Serious errors requiring immediate attention
# Set `LOG_LEVEL` to one of the values above (case-sensitive).
# Default recommended: INFO
LOG_LEVEL=INFO

# Explicit LLM model to use for OpenAI and other LLM wrappers
# Default: gpt-5-mini (recommended)
LLM_MODEL=gpt-5-mini

# Docker Compose Configuration
# Main container settings
CONTAINER_NAME=mcp-browser-use-server
HOST_PORT=8081
VNC_HOST_PORT=5901

# Development container settings
DEV_CONTAINER_NAME=mcp-browser-use-dev-server
DEV_HOST_PORT=8002
DEV_VNC_HOST_PORT=5902

# Proxy settings
PROXY_CONTAINER_NAME=mcp-browser-use-proxy
PROXY_PORT=9000
PROXY_TARGET_HOST=mcp-browser-use-server
PROXY_TARGET_PORT=8081

# Monitoring settings
PROMETHEUS_CONTAINER_NAME=mcp-browser-use-prometheus
PROMETHEUS_PORT=9090
GRAFANA_CONTAINER_NAME=mcp-grafana
GRAFANA_PORT=3000
GRAFANA_USER=admin
GRAFANA_PASSWORD=admin

# Docker network subnet
DOCKER_SUBNET=172.21.0.0/16
